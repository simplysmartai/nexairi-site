{
  "name": "nexairi-redesign",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate": "node scripts/auto-generate.js",
    "update-posts-index": "tsx scripts/updatePostsIndex.ts",
    "validate-posts": "tsx scripts/validatePosts.ts",
    "generate-post": "tsx scripts/generatePost.ts",
    "generate-batch": "tsx scripts/generateBatch.ts",
    "sync-frontmatter": "tsx scripts/syncFrontmatter.ts",
    "enforce-affiliates": "tsx scripts/enforceAffiliateCompliance.ts",
    "backfill-images": "tsx scripts/backfillImages.ts",
    "test-env": "tsx scripts/test-env.ts",
    "research": "tsx scripts/researchTopic.ts",
    "research-all": "npm run research -- --json",
    "seo-optimize": "tsx scripts/seoOptimizer.ts",
    "seo-all": "npm run seo-optimize -- --all",
    "review-queue": "tsx scripts/humanReviewQueue.ts",
    "draft-queue": "npm run schedule-agent -- --plan",
    "image-agent": "tsx scripts/imageGenerator.ts",
    "schedule-agent": "tsx scripts/weeklySchedule.ts",
    "support-router": "tsx scripts/supportRouter.ts",
    "email-agent": "tsx scripts/emailAgent.ts",
    "publish-agent": "tsx scripts/publishAgent.ts",
    "publish-all": "npm run publish-agent -- --auto",
    "analytics-agent": "tsx scripts/analyticsAgent.ts",
    "orchestra:week": "npm run research-all && npm run draft-queue && npm run review-queue",
    "orchestra:publish": "npm run seo-all && npm run publish-all",
    "orchestra:full": "npm run orchestra:week && powershell -Command \"Start-Sleep -Seconds 259200\" && npm run orchestra:publish"
    ,
    "ingest:article": "tsx scripts/ingest-article.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.77",
    "@ai-sdk/perplexity": "^2.0.21",
    "@google/genai": "*",
    "ai": "^5.0.106",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.294.0",
    "openai": "^6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "yaml": "^2.8.2"
  }
}
